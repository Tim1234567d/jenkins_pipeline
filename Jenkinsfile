    agent any 
    stages {
        stage('Build') { 
            steps {
                echo "Build project"
            }
        }
        stage('Unit-Test') { 
            steps {
                echo "Testing"
            }
        }
        stage('Deploy to dev') { 
            steps {
                echo "Deploy to dev"
            }
        }
        stage('Deploy to prod') { 
            steps {
                echo "Pipeline fineshed"
            }
        }
    }
}
